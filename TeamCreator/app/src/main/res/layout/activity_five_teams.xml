<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:background="@drawable/game_fem_lag"
    tools:context="com.example.golvmopp.teamcreator.FiveTeams">

    <ListView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/listView_Team1"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginRight="230dp"
        android:layout_marginEnd="252dp"
        android:layout_marginTop="120dp"
        android:layout_marginLeft="5dp"
        android:layout_marginBottom="300dp" />

    <ListView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/listView_Team2"
        android:layout_alignTop="@+id/listView_Team1"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginLeft="220dp"
        android:layout_marginRight="10dp"
        android:layout_marginBottom="300dp" />

    <ListView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/listView_Team3"
        android:layout_marginTop="150dp"
        android:layout_alignTop="@+id/listView_Team1"
        android:layout_centerHorizontal="true"
        android:layout_marginRight="200dp"
        android:layout_marginLeft="25dp"
        android:layout_marginBottom="160dp" />

    <ListView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/listView_Team4"
        android:layout_marginBottom="10dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="410dp"
        android:layout_marginRight="225dp"
        android:layout_marginLeft="5dp" />

    <ListView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/listView_Team5"
        android:layout_alignBottom="@+id/listView_Team4"
        android:layout_alignLeft="@+id/listView_Team2"
        android:layout_alignStart="@+id/listView_Team2"
        android:layout_marginTop="410dp"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp" />

</RelativeLayout>
