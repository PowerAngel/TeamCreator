<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:background="@drawable/game_sex_lag"
    tools:context="com.example.golvmopp.teamcreator.SixTeams">

    <ListView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/listView_Team1"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginRight="225dp"
        android:layout_marginEnd="234dp"
        android:layout_marginTop="100dp"
        android:layout_marginBottom="280dp"
        android:layout_marginLeft="10dp" />

    <ListView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/listView_Team2"
        android:layout_marginLeft="215dp"
        android:layout_alignTop="@+id/listView_Team1"
        android:layout_alignLeft="@+id/listView_Team1"
        android:layout_alignStart="@+id/listView_Team1"
        android:layout_marginStart="215dp"
        android:layout_marginBottom="280dp"
        android:layout_marginRight="10dp" />

    <ListView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/listView_Team3"
        android:layout_alignTop="@+id/listView_Team1"
        android:layout_alignRight="@+id/listView_Team1"
        android:layout_alignEnd="@+id/listView_Team1"
        android:layout_marginTop="150dp"
        android:layout_marginBottom="140dp"
        android:layout_marginRight="0dp"
        android:layout_marginLeft="10dp" />

    <ListView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/listView_Team4"
        android:layout_alignTop="@+id/listView_Team3"
        android:layout_alignLeft="@+id/listView_Team2"
        android:layout_alignStart="@+id/listView_Team2"
        android:layout_marginBottom="140dp"
        android:layout_marginTop="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="10dp" />

    <ListView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/listView_Team5"
        android:layout_marginBottom="0dp"
        android:layout_alignParentBottom="true"
        android:layout_alignRight="@+id/listView_Team3"
        android:layout_alignEnd="@+id/listView_Team3"
        android:layout_marginTop="400dp"
        android:layout_marginLeft="10dp" />

    <ListView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/listView_Team6"
        android:layout_alignTop="@+id/listView_Team5"
        android:layout_alignLeft="@+id/listView_Team4"
        android:layout_alignStart="@+id/listView_Team4"
        android:layout_marginRight="10dp" />
</RelativeLayout>
